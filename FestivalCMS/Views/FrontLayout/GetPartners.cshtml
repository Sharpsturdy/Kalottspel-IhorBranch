@model ArticlesFrontModel
@for (int i = 0; i < Model.Articles.Count; i++)
{
    <div class='img_block col-xs-12 col-sm-6 col-md-4 col-lg-2'>
        <a href="@Model.Articles[i].ExtLink" target="_blank"><img class='img-responsive center-block' src="/Images/@Model.Photos[Model.Articles[i].ID]"></a>
    </div>
}
